{
  "extends": "standard",
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "settings": {
    "import/core-modules": ["aws-sdk"]
  },
  "rules": {
    "comma-dangle": [
      "error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ]
  }
}
